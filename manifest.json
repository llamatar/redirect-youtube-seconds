{
  "manifest_version": 3,
  "name": "redirect-youtube-shorts",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "resources": [
        "content.js"
      ]
    }
  ]
}